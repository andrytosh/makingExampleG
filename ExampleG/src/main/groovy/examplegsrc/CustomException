package examplegsrc

class CustomException{
    def code
    def message

    CustomException(def code, def message) {
        this.code = code
        this.message = message
    }

    def getCode() {
        return code
    }

    def setCode(def code) {
        this.code = code
    }

    def getMessage() {
        return message
    }

    def setMessage(def message) {
        this.message = message
    }
}